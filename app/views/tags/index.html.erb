<head>
  <link href='http://fonts.googleapis.com/css?family=Patrick+Hand+SC' rel='stylesheet' type='text/css'>
</head>
<body>
  <h1>Tags</h1>
    <% @tags.each do |tag| %>
      <p><%= link_to tag.name.titleize, tag_path(tag) %></p>
    <% end %>
</body>

<style type="text/css">
h1 {
  font-family: 'Patrick Hand SC', cursive;
  color :black;
}
p {
  font-size: 16px;
  font-family: arial;
}
</style>
